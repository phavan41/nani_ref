{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "aljvehicle"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
		"images",
        "favicon.ico",
		".htaccess"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
		"assets/css/vendors.bundle.css",
		"assets/css/style.bundle.css",
        "styles.css",
        "assets/css/flaticon/css/flaticon.css",
        "assets/css/toastr.css",
        "assets/css/font-awesome.css",
		"assets/css/map-icons.css",
    "assets/css/custom.css",
    "assets/assiests/cookie-law-info-gdpr.css"
      ],
      "scripts": [
        "assets/js/jquery-3.2.1.js",
		"assets/js/vendors.bundle.js",
		"assets/js/scripts.bundle.js",
		"assets/js/select2.js",
        "assets/js/toastr.min.js",
		"assets/js/bootstrap-datepicker.js",
		"assets/js/dropzone.js",
		"assets/js/map-icons.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
		"staging": "environments/environment.staging.ts",
		"testing": "environments/environment.testing.ts",
		"fleetpath": "environments/environment.fleetpath.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
